   "source": [
    "nums = [3,2,2,3]\n",
    "val = 3\n",
    "\n",
    "def removeElement(nums, val):\n",
    "    i = len(nums) - 1\n",
    "    while i >= 0:\n",
    "        if nums[i] == val:\n",
    "            nums.pop(i)\n",
    "        i -= 1\n",
    "    return len(nums), nums\n",
    "\n",
    "removeElement(nums, val)"
   ]
