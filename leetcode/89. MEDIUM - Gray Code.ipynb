    "n = 3\n",
    "\n",
    "def gray_code(n):\n",
    "    if n == 1:\n",
    "        return ['0', '1']\n",
    "    else:\n",
    "        return ['0' + i for i in gray_code(n - 1)] + ['1' + i for i in gray_code(n - 1)[::-1]]\n",
    "    \n",
    "print(gray_code(n))"
