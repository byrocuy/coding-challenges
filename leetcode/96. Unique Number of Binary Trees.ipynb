    "n = 4\n",
    "\n",
    "def NumberOfTrees(n):\n",
    "    result = [1] * (n +1)\n",
    "    for i in range(2, n+1):\n",
    "        result[i] = 0\n",
    "        for j in range(i):\n",
    "            result[i] += result[j] * result[i-j-1]\n",
    "\n",
    "    return result[n]\n",
    "\n",
    "print(NumberOfTrees(n))"
   ]
