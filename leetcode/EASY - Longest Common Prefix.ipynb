   "source": [
    "strs = [\"\"]\n",
    "\n",
    "def longestCommonPrefix(strs):\n",
    "    shortest = min(strs, key=len)\n",
    "    for i, ch in enumerate(shortest):\n",
    "        for st in strs:\n",
    "                if ch != st[i]:\n",
    "                    return shortest[:i]\n",
    "    return shortest"
   ]
